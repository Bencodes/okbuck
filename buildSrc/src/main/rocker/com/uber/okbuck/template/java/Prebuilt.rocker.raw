@args (
String mavenCoords,
String sha256,
String sourcesMavenCoords,
String sourcesSha256,
)
okbuck_prebuilt(
    name = '@name',
    maven_coords = '@mavenCoords',
    sha256 = '@sha256',
@if (valid(sourcesMavenCoords)) {
    sources_maven_coords = '@sourcesMavenCoords',
    sources_sha256 = '@sourcesSha256',
}
)
